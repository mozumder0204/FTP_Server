			<th>Request ID</th>
			<th>Request Time</th>
            <th>Request Text</th>
            <th>Action</th>
		</tr>
		<tr>
        <% for(var i=0; i < qList.length; i++){ %>
			<td><%=qList[i].R_ID%></td>
            <td><%=qList[i].R_TIME%></td>
            <td><%=qList[i].R_TEXT%></td>
			<td>
				<a href="/admin/requestBox/<%=qList[i].R_ID%>">Delete</a> 
           </td>
         </tr>
         <% } %>
    </table>
    
</div>
</form>
